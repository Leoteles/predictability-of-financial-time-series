close all;
clear all;
clc;

%dimensão da reta

N = 100;
y = 1:0.1:N;%reta
epsilon = 0.1:0.1:10;%Valores de distância para os quais os pontos são considerados vizinhos
vizinhos = zeros(length(y),1);

for e = 1:length(epsilon)
    for i = 1:length(y)
        for j = 1:length(y)
            if i~=j
                d = abs(y(i)-y(j));%Distancia
                if (d<=epsilon(e))
                    vizinhos(i) = vizinhos(i) + 1;
                end
            end
        end
    end
    
end

